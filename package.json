{
  "name": "subscroller.com",
  "version": "1.0.0",
  "description": "",
  "main": "src/client/index.jsx",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "emotion",
      "@babel/plugin-transform-react-jsx-source"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "./config/enzyme.js"
    ]
  },
  "scripts": {
    "start:proxy": "PORT=8005 NODE_ENV=production node ./App",
    "dev": "concurrently \"yarn dev:proxy\" \"yarn dev:app\"",
    "dev:proxy": "PORT=9005 NODE_ENV=development node src/proxy/App",
    "dev:app": "webpack-dev-server -d --config config/webpack.dev.config.js",
    "build": "webpack -p --config config/webpack.prod.config.js",
    "test": "jest --colors",
    "test:coverage": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@reach/router": "^1.2.1",
    "moment": "^2.22.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-masonry-infinite": "^1.2.2",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-emotion": "^10.0.7",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.1.0",
    "normalize.css": "^8.0.1",
    "react-icons": "^3.2.1",
    "react-masonry-component": "^6.2.1",
    "react-test-renderer": "^16.5.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
